<!DOCTYPE html>
<html>
  <head>
    <title>Pairing Reviews</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body>
    <div class="topnav" id="myTopnav">
      <%= render "layouts/header" %>
      <%if user_signed_in? %>
        <%= render "layouts/menu" %>
      <%end %>
    </div>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>


